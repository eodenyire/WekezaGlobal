{
  "id": null,
  "uid": "wgi-compliance",
  "title": "WGI Compliance & AML",
  "description": "KYC status, AML alert volumes, and regulatory compliance monitoring — Architecture §9",
  "tags": ["wgi", "compliance", "aml", "kyc"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-3h", "to": "now" },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "AML Alerts Raised (last 5m)",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 20 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "increase(http_requests_total{route=~\"/v1/aml.*\",status=\"201\"}[5m])",
          "legendFormat": "AML Alerts",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "AML Scan Requests (last 5m)",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "increase(http_requests_total{route=\"/v1/aml/scan\",status=\"200\"}[5m])",
          "legendFormat": "Scans",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "stat",
      "title": "KYC Document Uploads (last 5m)",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "increase(http_requests_total{route=\"/v1/kyc\",status=\"201\"}[5m])",
          "legendFormat": "KYC Uploads",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Regulatory Reports Generated (last 1h)",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }, "unit": "short" },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "increase(http_requests_total{route=\"/v1/kyc/reports\",status=\"201\"}[1h])",
          "legendFormat": "Reports",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "AML Alert Rate (alerts/min)",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "short", "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 20, "spanNulls": false } },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "rate(http_requests_total{route=~\"/v1/aml.*\",status=\"201\"}[2m]) * 60",
          "legendFormat": "AML Alerts/min",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "KYC Document Processing Rate",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "reqps", "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 15, "spanNulls": false } },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "rate(http_requests_total{route=~\"/v1/kyc.*\",status=\"200\"}[2m])",
          "legendFormat": "KYC reads",
          "refId": "A"
        },
        {
          "datasource": "Prometheus",
          "expr": "rate(http_requests_total{route=~\"/v1/kyc.*\",status=\"201\"}[2m])",
          "legendFormat": "KYC writes",
          "refId": "B"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "AML Alert Resolution Rate",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "reqps", "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "spanNulls": false } },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "rate(http_requests_total{route=~\"/v1/aml/alerts/.*/resolve\",status=\"200\"}[2m])",
          "legendFormat": "Resolutions",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Compliance API Latency p95",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "unit": "s", "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10, "spanNulls": false } },
        "overrides": []
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{route=~\"/v1/kyc.*\"}[5m]))",
          "legendFormat": "KYC p95",
          "refId": "A"
        },
        {
          "datasource": "Prometheus",
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{route=~\"/v1/aml.*\"}[5m]))",
          "legendFormat": "AML p95",
          "refId": "B"
        }
      ]
    }
  ]
}
